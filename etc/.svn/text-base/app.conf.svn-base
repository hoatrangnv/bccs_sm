##################################################
# Config Threads (receiver, process, sender)


# Application name, ma ung dung trong CONFIG
APP_ID=PAYBONUS

# Other config
# Time sleep when no record( miliseconds )
PROCESS_SLEEP = 1000
# Number rows process in once
MAX_ROW = 100

# Enable using Exchange
EXCHANGE_ENABLE=true

# Enable using SMSWS
SMSWS_ENABLE=false
#DELETE_QUEUE_INPUT_TYPE 1 is alway delete queue input, 2 is only delete for successfull record or over retry count, 0 is alway not delete
DELETE_QUEUE_INPUT_TYPE = 1

MAX_RETRY_QUEUE_INPUT=0

INSERT_QUEUE_OUTPUT=true

UPDATE_SQL_MO_PARAM = false